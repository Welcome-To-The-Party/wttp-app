{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      ["module:react-native-dotenv", {
        "moduleName": "@env",
        "path": ".env"
      }],
      [
        "module-resolver",
        {
          "root": ["./src"],
          "extensions": [
            ".js",
            ".json"
          ],
          "alias": {
            "@navigations": "./src/navigations",
            "@components": "./src/components",
            "@assets": "./src/assets",
            "@screens": "./src/screens",
            "@styles": "./src/styles",
            "@store": "./src/store"
          }
        }
      ]
    ]
  }